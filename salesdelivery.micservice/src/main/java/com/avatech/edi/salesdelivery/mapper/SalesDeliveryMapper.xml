    <insert id="insertSalesDelivery" parameterType="com.avatech.edi.salesdelivery.model.bo.salesdelivery.SalesDelivery">
         INSERT INTO "AVA_IM_ODRN" (
                 "EDIDocEntry",
                 "DocEntry",
                 "CardCode",
                 "CardName",
                 "ShiptoCode",
                 "Address3",
                 "Phone1",
                 "CntctCode",
                 "DocType",
                 "DocDate",
                 "DocStatus",
                 "Comments",
                 "UserSign",
                 "PayType",
                 "DeliveryNum",
                 "DocTime",
                 "ErrorTime",
                 "Udf1",
                 "Udf2",
                 "Udf3",
                 "Udf4",
                 "Udf5"
          )
         values(
                  #{eDIDocEntry},
                  #{docEntry},
                  #{cardCode},
                  #{cardName},
                  #{shiptoCode},
                  #{address3},
                  #{phone1},
                  #{cntctCode},
                  #{docType},
                  #{docDate},
                  #{docStatus},
                  #{comments},
                  #{userSign},
                  #{payType},
                  #{deliveryNum},
                  #{docTime},
                  #{errorTime},
                  #{udf1},
                  #{udf2},
                  #{udf3},
                  #{udf4},
                  #{udf5}
         )
    </insert>

    <select id="searchSalesDeliverys" resultMap="SalesDeliveryMap">
         SELECT
                 T0."EDIDocEntry",
                 T0."DocEntry",
                 T0."CardCode",
                 T0."CardName",
                 T0."ShiptoCode",
                 T0."Address3",
                 T0."Phone1",
                 T0."CntctCode",
                 T0."DocType",
                 T0."DocDate",
                 T0."DocStatus",
                 T0."Comments",
                 T0."UserSign",
                 T0."PayType",
                 T0."DeliveryNum",
                 T0."DocTime",
                 T0."ErrorTime",
                 T0."Udf1",
                 T0."Udf2",
                 T0."Udf3",
                 T0."Udf4",
                 T0."Udf5"
         from "AVA_IM_ODRN" T0 WHERE "IsSync" = 'N'
    </select>

