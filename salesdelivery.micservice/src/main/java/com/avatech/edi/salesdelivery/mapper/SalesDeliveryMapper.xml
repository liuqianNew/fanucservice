    <update id="updateSalesDelivery" parameterType="SalesDeliveryMap">
        UPDATE "AVA_IM_ODRN" SET "IsSync" = #{isSync}
                                 ,"SyncMessage" = #{syncMessage}
                                 ,"SyncDate" = #{syncDate}
                                 ,"SyncTime" = #{syncTime}
                                 ,"ErrorTime" = #{errorTime}
                                 ,"SAPDocEntry" = #{sapDocEntry}
                                 WHERE "EDIDocEntry" = #{eDIDocEntry}
