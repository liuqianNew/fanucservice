    <select id="searchSalesDeliveryItems" resultMap="SalesDeliveryItemMap" parameterType="Long">
         SELECT
                 T0."EDIDocEntry",
                 T0."EDILineId",
                 T0."DocEntry",
                 T0."LineId",
                 T0."ItemCode",
                 T0."ItemName",
                 T0."Quantity",
                 T0."WhsCode",
