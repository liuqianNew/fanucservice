         INSERT INTO "AVA_IM_DRN2" (
                 "EDIDocEntry",
                 "EDILineId",
                 "EDIItemLineId",
                 "DocEntry",
                 "ItemLineId",
                 "LineNum",
                 "ItemCode",
                 "Quantity",
